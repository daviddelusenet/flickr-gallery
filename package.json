{
  "name": "flickr-gallery",
  "version": "1.0.0",
  "description": "A Flickr image gallery made with React",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build:dev",
    "build:dev": "BABEL_ENV=dev webpack-dev-server --hot --config config/webpack.dev.js --mode development",
    "build:prod": "BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production",
    "dist": "npm run build:prod",
    "dist:analyze": "BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production --env.analyze=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daviddelusenet/flickr-gallery.git"
  },
  "keywords": [
    "Flickr",
    "image",
    "gallery",
    "React"
  ],
  "author": "David de Lusenet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daviddelusenet/flickr-gallery/issues"
  },
  "homepage": "https://github.com/daviddelusenet/flickr-gallery#readme",
  "browserslist": [
    "Last 1 Chrome versions",
    "Last 1 Edge versions",
    "Last 1 Firefox versions"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "styled-components": "^4.2.0",
    "styled-reset": "^2.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^2.0.2",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "stylelint-config-recommended": "^2.2.0",
    "webpack": "^4.31.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
